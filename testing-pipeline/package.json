{
  "name": "communitymgt-iteratemarketing",
  "version": "1.0.0",
  "description": "Standalone testing suite",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf target",
    "serenity:update": "serenity update",
    "webdriver:update": "webdriver-manager update",
    "pree2e": "npm run clean && npm run serenity:update && npm run webdriver:update",
    "e2e": "protractor protractor.conf.js",
    "e2e-browserstack": "protractor browserstack.conf.js",
    "e2e-tag": "npm run e2e -- --cucumberOpts.tags",
    "e2e-scenario": "npm run e2e -- --cucumberOpts.name",
    "report": "serenity run"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/node": "^8.0.40",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-smoothie": "^0.3.2",
    "cucumber": "^1.3.2",
    "npm-failsafe": "^0.2.1",
    "open": "0.0.5",
    "opn-cli": "^3.1.0",
    "protractor": "^5.1.2",
    "rimraf": "^2.6.2",
    "serenity-cli": "^0.7.0",
    "serenity-js": "^1.9.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {}
}
